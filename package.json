{
	"name": "e2e-testing-with-selenium-js",
	"ava": {
		"verbose": true,
		"color": true,
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	},
	"version": "1.0.0",
	"description": "use selenium with ava",
	"main": "index.js",
	"scripts": {
		"start_selenium": "selenium-standalone install && selenium-standalone start",
		"test_ui_chrome": "BROWSER=chrome node_modules/.bin/ava tests/e2e_ui/*.ts",
		"test_ui_firefox": "BROWSER=firefox node_modules/.bin/ava tests/e2e_ui/*.ts"
	},
	"author": "Hung Tran",
	"license": "ISC",
	"dependencies": {
		"@types/node": "^12.6.2",
		"@types/selenium-webdriver": "^4.0.1",
		"ava": "^2.2.0",
		"axios": "^0.21.1",
		"chromedriver": "^75.0.1",
		"faker": "^4.1.0",
		"geckodriver": "^1.16.2",
		"selenium-standalone": "^6.16.0",
		"selenium-webdriver": "^4.0.0-alpha.4",
		"ts-node": "^8.3.0",
		"typescript": "^3.5.3"
	}
}
